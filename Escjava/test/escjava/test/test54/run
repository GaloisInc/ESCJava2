${ESCJ} ${ARGS} Simple2.java
${ESCJ} ${ARGS} Constructor.java
${ESCJ} ${ARGS} chain0.java
${ESCJ} ${ARGS} chain1.java
${ESCJ} ${ARGS} chain2.java
${ESCJ} ${ARGS} abstract.java
