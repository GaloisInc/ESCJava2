#!/bin/csh -f

rm -rf summary
cat > summary <<EOF
****									 ****
**** This file is automatically generated by make_summary; do not hand   ****
**** edit it!								 ****
****									 ****

EOF

foreach i (test? test?? test??? test???? test?????)
  echo -n "."
  echo -n `echo "${i}:" | sed 's/test//'` >> summary
  grep "##" $i/run | sed 's/^.*##/	/' >> summary
end

echo
